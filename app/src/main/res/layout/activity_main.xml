<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hikebackground"
    tools:context="com.pakhigupta.hikerswatch.MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="61dp"
        android:text="Hiker's Watch"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/latTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:layout_marginStart="74dp"
        android:layout_marginTop="66dp"
        android:textColor="@color/white"
        android:textSize="21sp"
        android:text="" />

    <TextView
        android:id="@+id/lngTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/latTextView"
        android:layout_marginStart="74dp"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="21sp"
        android:text="" />

    <TextView
        android:id="@+id/altTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/lngTextView"
        android:layout_marginStart="74dp"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="21sp"
        android:text="" />

    <TextView
        android:id="@+id/accTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/altTextView"
        android:layout_marginStart="74dp"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="21sp"
        android:text="" />

    <TextView
        android:id="@+id/addressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/accTextView"
        android:layout_marginStart="74dp"
        android:layout_marginTop="15dp"
        android:textColor="@color/white"
        android:textSize="21sp"
        android:text="" />


</RelativeLayout>
